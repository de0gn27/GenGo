{
  "name": "Google Search Language Switcher",
  "description": "Quickly switch Google Search results language without having to go to settings every time.",
  "version": "0.1.0",
  "manifest_version": 3,
  "icons": {
    "16": "./icons/icon16.png",
    "32": "./icons/icon32.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search*"],
      "js": ["./dist/content.js"]
    }
  ]
}
